             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="Semana3TagliacozzoNR.MainPage">

    <StackLayout>
        <!-- Place new controls here -->

        <Frame>

            <Label Text="Semana3">
                
            </Label>
            
        </Frame>


        <Entry x:Name="txtUser" Placeholder="Ingrese el usuario"/>
        <Entry x:Name="txtPass" Placeholder="Ingrese el Clave" IsPassword="True"/>


        <Button x:Name="btnAbrir"
                Text="Abrir ventana dos"
                Clicked="btnAbrir_Clicked">
            
        </Button>
        
    </StackLayout>

</ContentPage>
